{
  "configurations": [
    {
      "name": "Linux",
      "defines": [
        "NDEBUG",
        "_FORTIFY_SOURCE=2"
      ],
      "includePath": [
        "${workspaceFolder}/src",
        "/usr/local/lib/R/include",
        "/usr/local/include",
        "/usr/local/lib/R/site-library/Rcpp/include"
      ],
      "compilerPath": "/usr/bin/g++",
      "cStandard": "${default}",
      "cppStandard": "gnu++17",
      "intelliSenseMode": "linux-gcc-x64"
    }
  ],
  "version": 4
}
